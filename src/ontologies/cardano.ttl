    rdfs:label "Apache License v2" ;
    dc:title "Apache License, Version 2.0" .


# Core Blockchain Classes
b:Blockchain a owl:Class ;
    rdfs:label "Blockchain" ;
    rdfs:comment "A distributed ledger recording transactions across a network" .

b:Block a owl:Class ;
    rdfs:label "Block" ;
    rdfs:comment "A collection of validated transactions in the blockchain" .

b:Transaction a owl:Class ;
    rdfs:label "Transaction" ;
    rdfs:comment "A record of data registration or value transfer between accounts" .

b:Node a owl:Class ;
    rdfs:label "Node" ;
    rdfs:comment "Computer participating in blockchain network validation" .

b:Token a owl:Class ;
    rdfs:label "Token" ;
    rdfs:comment "Digital asset representing value on blockchain" .

b:FungibleToken a owl:Class ;
    rdfs:label "Fungible Token" ;
    rdfs:comment "Interchangeable tokens of equal value" ;
    rdfs:subClassOf b:Token .

b:NFT a owl:Class ;
    rdfs:label "Non-Fungible Token" ;
    rdfs:comment "Unique digital asset" ;
    rdfs:subClassOf b:Token .

b:NativeToken a owl:Class ;
    rdfs:subClassOf b:FungibleToken ;
    rdfs:label "Native Token" ;
    rdfs:comment "A native token in a blockchain." .

b:Account a owl:Class ;
    rdfs:label "Account" ;
    rdfs:comment "Entity holding tokens and interacting with blockchain" .

b:Address a owl:Class ;
    rdfs:label "Address" ;
    rdfs:comment "Entity that is part of an account." .

b:TokenState a owl:Class ;
    rdfs:label "Token State" ;
    rdfs:comment "Describes the state of a specific token in an entity. E.g., account, amount, timestamp and currency" .

b:SmartContract a owl:Class ;
    rdfs:label "Smart Contract" ;
    rdfs:comment "Self-executing contract with terms written in code" ;
    rdfs:subClassOf b:Account .

b:Metadata a owl:Class ;
    rdfs:label "Metadata" ;
    rdfs:comment "Additional information attached to blockchain entities." .

b:OffChainData a owl:Class ;
    rdfs:label "Off-chain Data" ;
    rdfs:comment "Data stored outside the blockchain but referenced by it." .

# General Blockchain Consensus Classes
b:ConsensusProtocol a owl:Class ;
    rdfs:label "Consensus Protocol" ;
    rdfs:comment "The protocol governing how nodes reach agreement on the blockchain state, including rules for block production, validation, and chain selection." .

b:ConsensusState a owl:Class ;
    rdfs:label "Consensus State" ;
    rdfs:comment "The current state of the consensus protocol, including relevant metrics and parameters." .

b:ProofOfWork a owl:Class ;
    rdfs:subClassOf b:ConsensusProtocol ;
    rdfs:label "Proof of Work" ;
    rdfs:comment "A consensus mechanism where block production requires solving computationally intensive cryptographic puzzles." .

b:ProofOfStake a owl:Class ;
    rdfs:subClassOf b:ConsensusProtocol ;
    rdfs:label "Proof of Stake" ;
    rdfs:comment "A consensus mechanism where block production rights are proportional to staked tokens in the system." .

b:LeadershipProof a owl:Class ;
    rdfs:label "Leadership Proof" ;
    rdfs:comment "Cryptographic evidence demonstrating an entity's right to produce a block in consensus systems." .

b:ChainSelectionRule a owl:Class ;
    rdfs:label "Chain Selection Rule" ;
    rdfs:comment "The method used to determine the best chain when multiple valid chain options exist." .

b:ValidationRule a owl:Class ;
    rdfs:label "Validation Rule" ;
    rdfs:comment "A specific rule for verifying the validity of blocks and transactions." .

# Cardano core blockchain classes
c:TransactionInput a owl:Class ;
    rdfs:subClassOf b:Transaction ;
    rdfs:label "Transaction Input" ;
    rdfs:comment "An input to a transaction." .

c:CollateralInput a owl:Class ;
    rdfs:subClassOf c:TransactionInput ;
    rdfs:label "Collateral Input" ;
    rdfs:comment "Input provided as collateral for script execution." .

c:ReferenceInput a owl:Class ;
    rdfs:subClassOf c:TransactionInput ;
    rdfs:label "Reference Input" ;
    rdfs:comment "Input referenced but not spent by a transaction." .

c:TransactionOutput a owl:Class ;
    rdfs:subClassOf b:Transaction ;
    rdfs:label "Transaction Output" ;
    rdfs:comment "An output from a transaction." .

c:CollateralOutput a owl:Class ;
    rdfs:subClassOf c:TransactionOutput ;
    rdfs:label "Collateral Output" ;
    rdfs:comment "Output returned when script execution succeeds." .

c:TransactionWitness a owl:Class ;
    rdfs:label "Transaction Witness" ;
    rdfs:comment "Cryptographic proof authorizing transaction execution." .

c:KeyWitness a owl:Class ;
    rdfs:subClassOf c:TransactionWitness ;
    rdfs:label "Key Witness" ;
    rdfs:comment "Signature-based proof of authorization." .

c:ScriptWitness a owl:Class ;
    rdfs:subClassOf c:Script ;
    rdfs:label "Script Witness" ;
    rdfs:comment "Script-based proof of authorization." .

c:Slot a owl:Class ;
    rdfs:label "Slot" ;
    rdfs:comment "A unit of time in the Cardano blockchain." .

c:SlotLeader a owl:Class ;
    rdfs:label "Slot Leader" ;
    rdfs:comment "An entity authorized to produce blocks in a slot." .

c:ProtocolEra a owl:Class ;
    rdfs:label "Protocol Era" ;
    rdfs:comment "A major upgrade phase in Cardano's protocol evolution, each introducing features and capabilities. Examples include Byron (legacy), Shelley (decentralization), Allegra (token locking), Mary (multi-asset), Alonzo (smart contracts), Babbage (Plutus V2), and Conway (governance)" .

c:Certificate a owl:Class ;
    rdfs:label "Certificate" ;
    rdfs:comment "A certificate representing various blockchain operations like delegation, registration, and retirement" .

c:Delegation a owl:Class ;
    rdfs:label "Delegation" ;
    rdfs:comment "Delegation of power to an entity (e.g. StakePool, DRep, etc.)" .

c:ConstitutionalCommitteeDelegation a owl:Class ;
    rdfs:subClassOf c:Delegation ;
    rdfs:label "Constitutional Committee Delegation" ;
    rdfs:comment "Certificate for delegating constitutional committee authority" .

c:ConstitutionalCommitteeRetirement a owl:Class ;
    rdfs:subClassOf c:Certificate ;
    rdfs:label "Constitutional Committee Retirement" ;
    rdfs:comment "Certificate for retiring from constitutional committee" .

c:DelegateRepresentativeRegistration a owl:Class ;
    rdfs:subClassOf c:Certificate ;
    rdfs:label "DRep Registration" ;
    rdfs:comment "Certificate for registering as a delegate representative" .

c:DelegateRepresentativeUpdate a owl:Class ;
    rdfs:subClassOf c:Certificate ;
    rdfs:label "DRep Update" ;
    rdfs:comment "Certificate for updating delegate representative information" .

c:DelegateRepresentativeRetirement a owl:Class ;
    rdfs:subClassOf c:Certificate ;
    rdfs:label "DRep Retirement" ;
    rdfs:comment "Certificate for retiring as a delegate representative" .

# Staking and Pool Classes
c:StakePool a owl:Class ;
    rdfs:label "Stake Pool" ;
    rdfs:comment "An entity that participates in Cardano block production and network consensus." .

c:StakePoolRetirement a owl:Class ;
    rdfs:label "Stake Pool Retirement" ;
    rdfs:comment "A certificate announcing stake pool retirement" .

c:OffChainStakePoolData a owl:Class ;
    rdfs:subClassOf c:OffChainMetadata ;
    rdfs:label "Off-chain Stake Pool Data" ;
    rdfs:comment "Stake Pool specific metadata stored off-chain." .

# Epoch and Network Classes
c:Epoch a owl:Class ;
    rdfs:label "Epoch" ;
    rdfs:comment "A fixed period in the Cardano blockchain." .

c:EpochStake a owl:Class ;
    rdfs:label "Epoch Stake" ;
    rdfs:comment "The stake distribution within an epoch." .

c:EpochParameters a owl:Class ;
    rdfs:label "Epoch Parameters" ;
    rdfs:comment "Protocol parameters active during an epoch." .

c:EpochState a owl:Class ;
    rdfs:label "Epoch State" ;
    rdfs:comment "Governance and operational state within an epoch." .

c:AdaPots a owl:Class ;
    rdfs:label "Ada Pots" ;
    rdfs:comment "Different types of Ada reserves in the system." .

# Asset Classes
c:CNT a owl:Class ;
    rdfs:subClassOf b:NativeToken ;
    rdfs:label "Cardano Native Token" ;
    rdfs:comment "A native token in the Cardano blockchain." .

c:MultiAssetCNT a owl:Class ;
    rdfs:subClassOf c:CNT ;
    rdfs:label "Multi Asset Cardano Native Token" ;
    rdfs:comment "A native token with custom attributes." .

# Script Classes
c:Script a owl:Class ;
    rdfs:label "Script" ;
    rdfs:comment "A program controlling asset spending or contract execution." .

c:PlutusScript a owl:Class ;
    rdfs:subClassOf c:Script ;
    rdfs:label "Plutus Script" ;
    rdfs:comment "A smart contract written in Plutus." .

c:NativeScript a owl:Class ;
    rdfs:subClassOf c:Script ;
    rdfs:label "Native Script" ;
    rdfs:comment "A simple script using native operations." .

c:Datum a owl:Class ;
    rdfs:label "Datum" ;
    rdfs:comment "Data attached to script outputs." .

c:Redeemer a owl:Class ;
    rdfs:label "Redeemer" ;
    rdfs:comment "Input parameters for script execution." .

c:CostModel a owl:Class ;
    rdfs:label "Cost Model" ;
    rdfs:comment "Parameters defining execution costs for scripts." .

c:ExecutionUnits a owl:Class ;
    rdfs:label "Execution Units" ;
    rdfs:comment "Measures of script execution resource usage." .

# Governance Classes
c:Committee a owl:Class ;
    rdfs:label "Constitutional Committee" ;
    rdfs:comment "A group overseeing governance and constitutional matters." .

c:CommitteeMember a owl:Class ;
    rdfs:label "Committee Member" ;
    rdfs:comment "An individual member of the constitutional committee." .

c:CommitteeCredential a owl:Class ;
    rdfs:label "Committee Credential" ;
    rdfs:comment "Authentication credentials for committee members." .

c:HotCredential a owl:Class ;
    rdfs:subClassOf c:CommitteeCredential ;
    rdfs:label "Hot Credential" ;
    rdfs:comment "Online voting credentials for committee members." .

c:ColdCredential a owl:Class ;
    rdfs:subClassOf c:CommitteeCredential ;
    rdfs:label "Cold Credential" ;
    rdfs:comment "Offline administrative credentials for committee members." .

c:DRep a owl:Class ;
    rdfs:label "Delegated Representative" ;
    rdfs:comment "An entity that can vote on behalf of delegators." .

c:GovernanceProposal a owl:Class ;
    rdfs:label "Governance Proposal" ;
    rdfs:comment "A formal suggestion submitted by a participant to bring about a change or initiate an activity within the Cardano ecosystem. Proposals are the starting point in the governance lifecycle." .

c:GovernanceAction a owl:Class ;
    rdfs:label "Governance Action" ;
    rdfs:comment "An executable step that implements the decision made through the governance process. It is triggered after a governance proposal has been approved or rejected." .

c:Constitution a owl:Class ;
    rdfs:label "Constitution" ;
    rdfs:comment "The fundamental rules governing the Cardano network." .

c:Vote a owl:Class ;
    rdfs:label "Vote" ;
    rdfs:comment "A formal expression of opinion on governance actions." .

c:VotingProcedure a owl:Class ;
    rdfs:label "Voting Procedure" ;
    rdfs:comment "The process and rules for governance voting." .

# Protocol Parameter Classes
c:ProtocolParameters a owl:Class ;
    rdfs:label "Protocol Parameters" ;
    rdfs:comment "Configuration values controlling network behavior." .

c:NetworkParameters a owl:Class ;
    rdfs:subClassOf c:ProtocolParameters ;
    rdfs:label "Network Parameters" ;
    rdfs:comment "Parameters controlling network operations." .

c:EconomicParameters a owl:Class ;
    rdfs:subClassOf c:ProtocolParameters ;
    rdfs:label "Economic Parameters" ;
    rdfs:comment "Parameters controlling economic aspects." .

c:TechnicalParameters a owl:Class ;
    rdfs:subClassOf c:ProtocolParameters ;
    rdfs:label "Technical Parameters" ;
    rdfs:comment "Parameters controlling technical aspects." .

# Metadata and Off-chain Classes
c:TransactionMetadata a owl:Class ;
    rdfs:subClassOf b:Metadata ;
    rdfs:label "Transaction Metadata" ;
    rdfs:comment "Metadata attached to transactions." .

c:OffChainMetadata a owl:Class ;
    rdfs:subClassOf b:OffChainData ;
    rdfs:label "Off-chain Metadata" ;
    rdfs:comment "Metadata stored off-chain." .

c:OffChainVoteData a owl:Class ;
    rdfs:subClassOf c:OffChainMetadata ;
    rdfs:label "Off-chain Vote Data" ;
    rdfs:comment "Vote-specific metadata stored off-chain." .

c:VotingAnchor a owl:Class ;
    rdfs:label "Voting Anchor" ;
    rdfs:comment "Reference to off-chain governance metadata." .

c:RewardWithdrawal a owl:Class ;
    rdfs:label "Reward Withdrawal" ;
    rdfs:comment "A withdrawal of rewards from a stake address" .

c:PotTransfer a owl:Class ;
    rdfs:label "Pot Transfer" ;
    rdfs:comment "Transfer between treasury and reserves" .

c:ETLProgress a owl:Class ;
    rdfs:label "ETL Progress" ;
    rdfs:comment "Metadata about ETL pipeline progress" .

c:NetworkMetric a owl:Class ;
    rdfs:label "Network Metric" ;
    rdfs:comment "Aggregated metrics about the network" .

c:hasMetricType a owl:DatatypeProperty ;
    rdfs:label "has metric type" ;
    rdfs:comment "Type of network metric" ;
    rdfs:domain c:NetworkMetric ;
    rdfs:range xsd:string .

c:hasMetricValue a owl:DatatypeProperty ;
    rdfs:label "has metric value" ;
    rdfs:comment "Value of the network metric" ;
    rdfs:domain c:NetworkMetric ;
    rdfs:range xsd:decimal .

# Properties - Core Blockchain
b:hasTx a owl:ObjectProperty ;
    rdfs:label "has transaction" ;
    rdfs:comment "Relates a block with the transactions recorded in it." ;
    rdfs:domain b:Block ;
    rdfs:range b:Transaction .

b:hasAddress a owl:ObjectProperty ;
    rdfs:label "has address" ;
    rdfs:comment "Relates an account with an address." ;
    rdfs:domain b:Account ;
    rdfs:range b:Address .

b:hasId a owl:DatatypeProperty ;
    rdfs:label "has id" ;
    rdfs:comment "Relates an address with its id." ;
    rdfs:domain b:Address ;
    rdfs:range xsd:string .

c:hasType a owl:DatatypeProperty ;
    rdfs:label "has type" ;
    rdfs:comment "Relates an address with its type." ;
    rdfs:domain b:Address ;
    rdfs:range xsd:string .

c:hasRewardWithdrawal a owl:ObjectProperty ;
    rdfs:label "has reward withdrawal" ;
    rdfs:comment "Links an address to a reward withdrawal" ;
    rdfs:domain b:Address ;
    rdfs:range c:RewardWithdrawal .

c:hasOutput a owl:ObjectProperty ;
    rdfs:label "has output" ;
    rdfs:comment "Links an address to an output" ;
    rdfs:domain b:Address ;
    rdfs:range c:TransactionOutput .

b:hasAccountUTXO a owl:ObjectProperty ;
    rdfs:label "has account UTXO" ;
    rdfs:comment "Relates an account with its utxos." ;
    rdfs:domain b:Account ;
    rdfs:range b:Transaction .

b:hasAccountStatus a owl:DatatypeProperty ;
    rdfs:label "has account status" ;
    rdfs:comment "Relates an account with its current status." ;
    rdfs:domain b:Account ;
    rdfs:range xsd:string .

b:hasConsensusProtocol a owl:ObjectProperty ;
    rdfs:label "has consensus protocol" ;
    rdfs:comment "Relates the network to its active consensus protocol implementation." ;
    rdfs:domain b:Blockchain ;
    rdfs:range b:ConsensusProtocol .

b:hasNativeToken a owl:ObjectProperty ;
    rdfs:label "has native token" ;
    rdfs:comment "Relates the network to its native token." ;
    rdfs:domain b:Blockchain ;
    rdfs:range b:FungibleToken .

b:hasGenesisBlock a owl:ObjectProperty ;
    rdfs:label "has genesis block" ;
    rdfs:comment "Relates the network to its genesis block." ;
    rdfs:domain b:Blockchain ;
    rdfs:range b:Block .

b:hasCurrentBlockHeight a owl:DatatypeProperty ;
    rdfs:label "has current block height" ;
    rdfs:comment "Relates the network to its current block height." ;
    rdfs:domain b:Blockchain ;
    rdfs:range xsd:decimal .

b:hasPreviousBlock a owl:ObjectProperty ;
    rdfs:label "has previous block" ;
    rdfs:comment "Relates a block to its previous block." ;
    rdfs:domain b:Block ;
    rdfs:range b:Block .

b:hasNextBlock a owl:ObjectProperty ;
    rdfs:label "has next block" ;
    rdfs:comment "Relates a block to its next block." ;
    rdfs:domain b:Block ;
    rdfs:range b:Block .

b:usesSelectionRule a owl:ObjectProperty ;
    rdfs:label "uses selection rule" ;
    rdfs:comment "Relates a consensus protocol to its chain selection rule." ;
    rdfs:domain b:ConsensusProtocol ;
    rdfs:range b:ChainSelectionRule .

b:usesValidationRule a owl:ObjectProperty ;
    rdfs:label "uses validation rule" ;
    rdfs:comment "Relates a consensus protocol to its validation rules." ;
    rdfs:domain b:ConsensusProtocol ;
    rdfs:range b:ValidationRule .

b:hasLeadershipProof a owl:ObjectProperty ;
    rdfs:label "has leadership proof" ;
    rdfs:comment "Relates a block to its leadership proof." ;
    rdfs:domain b:Block ;
    rdfs:range b:LeadershipProof .

b:isActiveProtocol a owl:DatatypeProperty ;
    rdfs:label "is active protocol" ;
    rdfs:comment "Indicates whether a consensus protocol is currently active." ;
    rdfs:domain b:ConsensusProtocol ;
    rdfs:range xsd:boolean .

b:hasTokenName a owl:DatatypeProperty ;
    rdfs:label "has token name" ;
    rdfs:comment "The name of a token." ;
    rdfs:domain b:Token ;
    rdfs:range xsd:string .

b:hasTokenSymbol a owl:DatatypeProperty ;
    rdfs:label "has token symbol" ;
    rdfs:comment "The symbol of a token." ;
    rdfs:domain b:Token ;
    rdfs:range xsd:string .

b:hasTokenDescription a owl:DatatypeProperty ;
    rdfs:label "has token description" ;
    rdfs:comment "The description of a token." ;
    rdfs:domain b:Token ;
    rdfs:range xsd:string .

c:transferredIn a owl:ObjectProperty ;
    rdfs:label "transferred in" ;
    rdfs:comment "Links a token to a transaction where it was transferred" ;
    rdfs:domain b:Token ;
    rdfs:range b:Transaction .

b:hasNFTContent a owl:DatatypeProperty ;
    rdfs:label "has nft content" ;
    rdfs:comment "The address of a content of a non-fungible token." ;
    rdfs:domain b:NFT ;
    rdfs:range xsd:string .

b:hasTotalSupply a owl:DatatypeProperty ;
    rdfs:label "has total supply" ;
    rdfs:comment "The current total supply of a token." ;
    rdfs:domain b:Token ;
    rdfs:range xsd:decimal .

b:hasMaxSupply a owl:DatatypeProperty ;
    rdfs:label "has max supply" ;
    rdfs:comment "The maximum supply (cap) of a token." ;
    rdfs:domain b:Token ;
    rdfs:range xsd:decimal .

b:hasDenominationName a owl:DatatypeProperty ;
    rdfs:label "has denomination name" ;
    rdfs:comment "Reference the name of the fractional unit of a cryptocurrency. For instance, Lovelace." ;
    rdfs:domain b:FungibleToken ;
    rdfs:range xsd:string .

b:hasConversionRate a owl:DatatypeProperty ;
    rdfs:label "has conversion rate" ;
    rdfs:comment "A property that defines the conversion between an asset and its denomination. For instance, 1 ADA = 1,000,000 Lovelace." ;
    rdfs:domain b:FungibleToken ;
    rdfs:range xsd:decimal .

b:hasTokenState a owl:ObjectProperty ;
    rdfs:label "has token state" ;
    rdfs:comment "Associates an account or a transaction with a token state description" ;
    rdfs:domain b:Account, c:Transaction ;
    rdfs:range b:TokenState .

b:hasCurrency a owl:ObjectProperty ;
    rdfs:label "has currency" ;
    rdfs:comment "Qualifies the asset that the token state is describing" ;
    rdfs:domain b:TokenState ;
    rdfs:range b:Token .

b:hasValue a owl:DatatypeProperty ;
    rdfs:label "has value" ;
    rdfs:comment "The amount of tokens (currency) in a token state." ;
    rdfs:domain b:TokenState ;
    rdfs:range xsd:decimal .

b:hasTokenStateTimestamp a owl:DatatypeProperty ;
    rdfs:label "has token state timestamp" ;
    rdfs:comment "The timestamp of a token state." ;
    rdfs:domain b:TokenState ;
    rdfs:range xsd:dateTime .

b:hasHash a owl:DatatypeProperty ;
    rdfs:label "has hash" ;
    rdfs:comment "The cryptographic hash identifier of an entity." ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

b:hasTimestamp a owl:DatatypeProperty ;
    rdfs:label "has timestamp" ;
    rdfs:comment "The point in time of an event." ;
    rdfs:domain b:Block ;
    rdfs:range xsd:dateTime .

b:hasMetadataJSON a owl:DatatypeProperty ;
    rdfs:label "has metadata json" ;
    rdfs:comment "The JSON content of metadata as a string" ;
    rdfs:domain b:Metadata ;
    rdfs:range xsd:string .

b:hasMetadataCBOR a owl:DatatypeProperty ;
    rdfs:label "has metadata cbor" ;
    rdfs:comment "The CBOR content of metadata as a string" ;
    rdfs:domain b:Metadata ;
    rdfs:range xsd:string .

b:hasMetadataDecodedCBOR a owl:DatatypeProperty ;
    rdfs:label "has metadata decoded cbor" ;
    rdfs:comment "The decoded CBOR content of metadata as a string" ;
    rdfs:domain b:Metadata ;
    rdfs:range xsd:string .

c:hasEra a owl:ObjectProperty ;
    rdfs:label "belongs to era" ;
    rdfs:comment "Associates a block with its corresponding Cardano protocol era, establishing the context of features and capabilities available for that entity" ;
    rdfs:domain b:Block ;
    rdfs:range c:ProtocolEra .

# Properties - Transaction Related
b:hasTxID a owl:DatatypeProperty ;
    rdfs:label "has transaction id" ;
    rdfs:comment "Relates a transaction with its identifier." ;
    rdfs:domain b:Transaction ;
    rdfs:range xsd:string .

c:hasTxInput a owl:ObjectProperty ;
    rdfs:label "has tx input" ;
    rdfs:comment "Relates a transaction to its inputs." ;
    rdfs:domain b:Transaction ;
    rdfs:range c:TransactionInput .

c:hasTxInputCount a owl:DatatypeProperty ;
    rdfs:label "has tx input count" ;
    rdfs:comment "Relates a transaction with the number of inputs in it." ;
    rdfs:domain b:Transaction ;
    rdfs:range xsd:decimal .

c:hasTxOutput a owl:ObjectProperty ;
    rdfs:label "has output" ;
    rdfs:comment "Relates a transaction to its outputs." ;
    rdfs:domain b:Transaction ;
    rdfs:range c:TransactionOutput .

c:hasTxOutputCount a owl:DatatypeProperty ;
    rdfs:label "has tx output count" ;
    rdfs:comment "Relates a transaction with the number of outputs in it." ;
    rdfs:domain b:Transaction ;
    rdfs:range xsd:decimal .

c:hasTxOutputValue a owl:DatatypeProperty ;
    rdfs:label "has tx input value" ;
    rdfs:comment "Relates a transaction with the sum of values of its outputs." ;
    rdfs:domain b:Transaction ;
    rdfs:range xsd:decimal .

c:hasMintedAsset a owl:ObjectProperty ;
    rdfs:label "has minted asset" ;
    rdfs:comment "Relates a transaction to the asset it is minting." ;
    rdfs:domain b:Transaction ;
    rdfs:range b:Token .

c:hasBurnedAsset a owl:ObjectProperty ;
    rdfs:label "has burned asset" ;
    rdfs:comment "Relates a transaction to the asset it is burining." ;
    rdfs:domain b:Transaction ;
    rdfs:range b:Token .

c:hasMintedAmount a owl:DatatypeProperty ;
    rdfs:label "has minted amount" ;
    rdfs:comment "Relates a an asset with amount minted." ;
    rdfs:domain b:MultiAssetCNT ;
    rdfs:range xsd:decimal .

c:hasBurnedAmount a owl:DatatypeProperty ;
    rdfs:label "has burned amount" ;
    rdfs:comment "Relates a an asset with amount minted." ;
    rdfs:domain b:MultiAssetCNT ;
    rdfs:range xsd:decimal .

c:hasTxMetadata a owl:ObjectProperty ;
    rdfs:label "has transaction metadata" ;
