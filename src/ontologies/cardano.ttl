@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ns: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .

@prefix b: <https://mobr.ai/ont/blockchain#> .
@prefix c: <https://mobr.ai/ont/cardano#> .

<https://mobr.ai/ont/cardano#> a owl:Ontology ;
	owl:versionIRI <https://mobr.ai/ont/cardano/v0.1/> ;
	owl:imports <http://www.w3.org/2003/06/sw-vocab-status/ns> ;
	dc:creator "MOBR Systems" ;
	dc:title "An ontology for the Cardano ecosystem" ;
	vann:preferredNamespacePrefix "cardano" ;
	rdfs:comment "This ontology specification uses W3C RDF Schema and the Web Ontology Language. Its main source of information comes from Cardano official documentation and Ogmios schema." ;
	rdfs:seeAlso <https://github.com/mobr-ai/cap> ;
	owl:versionInfo "0.1.0" ;
	ns:term_status "unstable" ;
    dc:license <https://www.apache.org/licenses/LICENSE-2.0> .

<https://www.apache.org/licenses/LICENSE-2.0>
